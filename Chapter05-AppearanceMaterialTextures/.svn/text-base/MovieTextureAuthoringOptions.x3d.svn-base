            <Coordinate point='-2.4 -1.35 0 2.4 -1.35 0 2.4 1.35 0 -2.4 1.35 0'/>
            <TextureCoordinate USE='FullImageMapping'/>
          </IndexedFaceSet>
          <Appearance>
            <TextureTransform/>
            <MovieTexture DEF='X3dQuipMovieHighDefinition' repeatS='false' repeatT='false' url='"X3dQuipBrutzmanH264MPEG1Mash.mpg" "X3dQuipBrutzmanH264.mov" "http://X3dGraphics.com/examples/X3dForWebAuthors/Chapter05-AppearanceMaterialTextures/X3dQuipBrutzmanH264MPEG1Mash.mpg" "http://X3dGraphics.com/examples/X3dForWebAuthors/Chapter05-AppearanceMaterialTextures/X3dQuipBrutzmanH264.mov"'/>
          </Appearance>
        </Shape>
